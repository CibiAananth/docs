  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@suki.ai"
      }
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/SukiHQ",
